{
  "extends": [
    "config:base",
    ":disableRateLimiting",
    ":rebaseStalePrs",
    ":disableDependencyDashboard"
  ],
  "assignees": [
    "growse"
  ],
  "lockFileMaintenance": {
    "enabled": true,
    "automerge": true
  },
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch"],
      "matchCurrentVersion": "!/^0/",
      "automerge": true
    }
  ]
}
